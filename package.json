    "eslint-plugin-promise": "^3.6.0",
    "eslint-plugin-standard": "^3.0.1",
    "axios": "^0.19.0",
    "bootstrap-sass": "^3.3.7",
    "cross-env": "^5.0.1",
    "laravel-mix": "^1.0",
    "lodash": "^4.17.4"
  },
